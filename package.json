{
  "name": "muesli_next",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@editorjs/editorjs": "^2.24.3",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@intutable/core": "^0.1.6",
    "@intutable/database": "^1.4.1",
    "@intutable/http": "^0.1.5",
    "@intutable/http-cors": "^0.1.0",
    "@intutable/lazy-views": "^3.0.0",
    "@intutable/project-management": "^1.0.0",
    "@types/node": "^16.11.38",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "axios": "^0.27.2",
    "editorjs-alert": "^1.0.3",
    "file-dialog": "^0.0.8",
    "object-hash": "^3.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-editor-js": "^2.0.6",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "tailwindcss-scoped-groups": "^2.0.0",
    "ts-md5": "^1.2.11",
    "typescript": "^4.7.2",
    "web-vitals": "^2.1.4",
    "zod": "^3.17.3"
  },
  "scripts": {
    "build:db": "cd database/ && docker-compose build --pull",
    "start:db": "npm run build:db && cd database/ && docker-compose up",
    "reset-db": "cd database/ && docker-compose down && cd .. && docker volume rm database_db-data",
    "build:back": "cd src/backend/startup && tsc && cd ../web-app-gen-plugin && tsc && cd ../../..",
    "start:back": "node ./src/backend/startup/dist/index",
    "build:app": "react-scripts build",
    "start:app": "react-scripts start",
    "start": "concurrently --kill-others-on-fail -p \"{name} {time}|\" -t \"HH:mm:ss\" --names \"DATA,BACK,APP \" -c \"yellow,blue,magenta\" npm:start:db npm:start:back npm:start:app",
    "build": "concurrently \"npm:build:*\"",
    "dev": "npm run build:back && npm run build:db && npm run start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src && prettier --check .",
    "format": "eslint src --fix && prettier --write .",
    "prepare": "husky install",
    "doc": "npx typedoc src/index.tsx && xdg-open docs/index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@editorjs/header": "^2.6.2",
    "@editorjs/list": "^1.7.0",
    "@editorjs/table": "^2.0.2",
    "@editorjs/warning": "^1.2.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "editorjs-button": "^1.0.4",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "husky": "^8.0.0",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "tailwindcss": "^3.1.2",
    "typedoc": "^0.22.17"
  }
}
